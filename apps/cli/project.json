{
  "name": "cli",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cli",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/cli"],
      "inputs": [
        "{projectRoot}/**/*",
        "!{projectRoot}/test/*",
        "!{projectRoot}/node_modules/*",
        "{workspaceRoot}/libs/interface/asset/src/**/*.ts",
        "{workspaceRoot}/libs/interface/asset/index.ts",
        "{workspaceRoot}/libs/interface/function/src/**/*.ts",
        "{workspaceRoot}/libs/interface/function/index.ts"
      ],
      "options": {
        "commands": [
          "tsc --project apps/cli/tsconfig.app.json",
          "node apps/cli/copy-bin.js",
          "resolve-tspaths"
        ],
        "parallel": false
      },
      "cache": true
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": [
          "node dist/apps/cli/bin/spica project sync --modules function --source-url http://localhost:4500/api --source-apikey 40599w1m7yoaf73 --target-url http://localhost:49613/api --target-apikey 4059a01m7yocd4v"
        ]
      }
    },
    "set-version": {}
  }
}
